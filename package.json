{
  "name": "freg",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.18.3",
  "description": "A minimal CLI to pull OCI/Docker images from registries and save them as tar files.",
  "author": "jinghaihan",
  "license": "MIT",
  "homepage": "https://github.com/jinghaihan/freg#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jinghaihan/freg.git"
  },
  "bugs": {
    "url": "https://github.com/jinghaihan/freg/issues"
  },
  "keywords": [
    "docker",
    "image",
    "download",
    "tar"
  ],
  "exports": {
    ".": "./dist/index.mjs",
    "./cli": "./dist/cli.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "freg": "./bin/freg.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "start": "tsx ./src/cli.ts",
    "build": "unbuild",
    "deps": "taze major -I",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "release": "bumpp",
    "bootstrap": "pnpm install",
    "prepare": "npx simple-git-hooks",
    "preinstall": "npx only-allow pnpm",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@clack/prompts": "catalog:node",
    "ansis": "catalog:node",
    "cac": "catalog:node",
    "pathe": "catalog:node",
    "tinyexec": "catalog:node",
    "unconfig": "catalog:node",
    "undici": "catalog:network"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:lint",
    "@antfu/utils": "catalog:utils",
    "@types/node": "catalog:types",
    "bumpp": "catalog:cli",
    "eslint": "catalog:lint",
    "lint-staged": "catalog:lint",
    "pncat": "catalog:cli",
    "simple-git-hooks": "catalog:lint",
    "taze": "catalog:cli",
    "tsx": "catalog:script",
    "typescript": "catalog:tsc",
    "unbuild": "catalog:build",
    "vitest": "catalog:test"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
